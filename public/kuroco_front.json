{
    "rewrites": [
        {
          "source": ".*",
          "destination": "/index.html"
        }
    ],
    "redirects": [
      {
        "source": "^/homePage",
        "destination": "/homePage/index.html"
      },
      {
        "source": "^/categories/([0-9]+)",
        "destination": "/categories/$1/index.html"
      },
      {
        "source": "^/categories/([0-9]+)/",
        "destination": "/categories/$1/index.html"
      }
  ]
}
